{
  "dashboard": {
    "title": "QFrame Trading System",
    "tags": ["qframe", "trading", "quantitative"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Portfolio Value Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "portfolio_total_value",
            "legendFormat": "Total Value"
          }
        ]
      },
      {
        "id": 2,
        "title": "Active Strategies",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(strategy_active_count)"
          }
        ]
      },
      {
        "id": 3,
        "title": "Trade Execution Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(trades_executed_total[5m])",
            "legendFormat": "Trades/sec"
          }
        ]
      },
      {
        "id": 4,
        "title": "Win Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(trades_winning_total) / sum(trades_total) * 100"
          }
        ],
        "options": {
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 40, "color": "yellow"},
              {"value": 60, "color": "green"}
            ]
          }
        }
      },
      {
        "id": 5,
        "title": "P&L Today",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "sum(pnl_realized_total{timeframe=\"1d\"})"
          }
        ]
      },
      {
        "id": 6,
        "title": "Open Positions",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "position_size_total",
            "format": "table"
          }
        ]
      },
      {
        "id": 7,
        "title": "Risk Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "risk_var_95",
            "legendFormat": "VaR 95%"
          },
          {
            "expr": "risk_max_drawdown",
            "legendFormat": "Max Drawdown"
          }
        ]
      },
      {
        "id": 8,
        "title": "System Health",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "up{job=\"qframe\"}"
          }
        ],
        "options": {
          "colorMode": "background",
          "mappings": [
            {"value": 1, "text": "Healthy", "color": "green"},
            {"value": 0, "text": "Down", "color": "red"}
          ]
        }
      },
      {
        "id": 9,
        "title": "API Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ]
      },
      {
        "id": 10,
        "title": "Event Bus Throughput",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(event_bus_published_total[5m])",
            "legendFormat": "Published/sec"
          },
          {
            "expr": "rate(event_bus_processed_total[5m])",
            "legendFormat": "Processed/sec"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "strategy",
          "type": "query",
          "query": "label_values(strategy_active_count, strategy_name)"
        },
        {
          "name": "symbol",
          "type": "query",
          "query": "label_values(position_size_total, symbol)"
        }
      ]
    },
    "time": {"from": "now-6h", "to": "now"},
    "refresh": "10s"
  }
}
