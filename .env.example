# QFrame Environment Variables Example
# ====================================
# Copiez ce fichier vers .env et remplissez avec vos vraies valeurs
# ATTENTION: Ne JAMAIS committer le fichier .env avec de vraies valeurs!

# === CONFIGURATION GÉNÉRALE ===

# Environnement de déploiement (development, testing, staging, production)
QFRAME_ENV=development

# Répertoire des fichiers de configuration
QFRAME_CONFIG_DIR=config

# Mode debug (true/false)
QFRAME_DEBUG=true

# Niveau de logging (DEBUG, INFO, WARNING, ERROR, CRITICAL)
QFRAME_LOG_LEVEL=INFO

# === BASE DE DONNÉES ===

# Host de la base de données PostgreSQL
QFRAME_DB_HOST=localhost

# Port de la base de données
QFRAME_DB_PORT=5432

# Nom de la base de données
QFRAME_DB_NAME=qframe

# Utilisateur de la base de données
QFRAME_DB_USER=qframe_user

# Mot de passe de la base de données (OBLIGATOIRE en production)
QFRAME_DB_PASSWORD=your_secure_database_password

# === REDIS ===

# Host Redis
QFRAME_REDIS_HOST=localhost

# Port Redis
QFRAME_REDIS_PORT=6379

# Mot de passe Redis (optionnel)
QFRAME_REDIS_PASSWORD=

# === API ET SÉCURITÉ ===

# Port de l'API REST
QFRAME_API_PORT=8000

# Clé secrète pour l'API (OBLIGATOIRE en production)
QFRAME_API_SECRET=your_super_secret_api_key_change_me_in_production

# Secret pour les JWT tokens (OBLIGATOIRE en production)
QFRAME_JWT_SECRET=your_super_secret_jwt_key_change_me_in_production

# Clé privée RSA pour JWT (optionnel, pour RS256)
QFRAME_JWT_PRIVATE_KEY=

# === TRADING - BINANCE ===

# Clés API Binance PRODUCTION (DANGER!)
BINANCE_API_KEY=your_binance_api_key_production_only
BINANCE_API_SECRET=your_binance_api_secret_production_only

# Clés API Binance TESTNET (pour développement/tests)
BINANCE_TESTNET_API_KEY=your_binance_testnet_api_key
BINANCE_TESTNET_API_SECRET=your_binance_testnet_api_secret

# === DONNÉES EXTERNES ===

# Clé API Alpha Vantage (données financières)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key

# Clé API Quandl (données économiques)
QUANDL_API_KEY=your_quandl_api_key

# Clé API News (flux d'actualités)
NEWS_API_KEY=your_news_api_key

# === CLOUD ET STORAGE ===

# AWS (pour S3, etc.)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_DEFAULT_REGION=us-east-1

# Google Cloud Platform
GCP_PROJECT_ID=your_gcp_project_id
GCP_SERVICE_ACCOUNT_KEY=path/to/your/gcp-service-account.json

# === MONITORING ET ALERTES ===

# Slack pour notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# PagerDuty pour alertes critiques
PAGERDUTY_INTEGRATION_KEY=your_pagerduty_integration_key

# Email SMTP
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_email_password

# === MLFLOW ===

# URI de tracking MLflow
QFRAME_MLFLOW_URI=http://localhost:5000

# Base de données MLflow
MLFLOW_DB_HOST=localhost
MLFLOW_DB_PASSWORD=your_mlflow_db_password

# Stockage d'artefacts MLflow
MLFLOW_ARTIFACT_ACCESS_KEY=your_mlflow_artifact_access_key

# === CHIFFREMENT ===

# Clé de chiffrement pour données sensibles (production)
QFRAME_ENCRYPTION_KEY=your_32_character_encryption_key_

# Clé de chiffrement pour sauvegardes (production)
QFRAME_BACKUP_ENCRYPTION_KEY=your_backup_encryption_key_here_

# === WEBHOOKS ===

# Secret pour valider les webhooks entrants
QFRAME_WEBHOOK_SECRET=your_webhook_validation_secret

# === LICENCE ===

# Clé de licence QFrame (si applicable)
QFRAME_LICENSE_KEY=your_qframe_license_key

# === HASHICORP VAULT (optionnel) ===

# URL du serveur Vault
VAULT_URL=http://localhost:8200

# Token d'authentification Vault
VAULT_TOKEN=your_vault_token

# === AZURE KEY VAULT (optionnel) ===

# URL du Azure Key Vault
AZURE_KEY_VAULT_URL=https://your-vault.vault.azure.net/

# === KUBERNETES (optionnel) ===

# Namespace Kubernetes
KUBERNETES_NAMESPACE=qframe

# === JAEGER TRACING (optionnel) ===

# Endpoint Jaeger pour tracing distribué
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# === DÉVELOPPEMENT UNIQUEMENT ===

# Activer le hot reload en développement
QFRAME_HOT_RELOAD=true

# Activer les endpoints de debug
QFRAME_DEBUG_ENDPOINTS=true

# Activer le profiling de performance
QFRAME_PROFILING=true

# Désactiver HTTPS en développement
QFRAME_DISABLE_HTTPS=true

# === TESTS UNIQUEMENT ===

# Mode test rapide
QFRAME_FAST_TESTS=true

# Activer les mocks pour services externes
QFRAME_MOCK_EXTERNAL_SERVICES=true

# Désactiver le nettoyage automatique des tests
QFRAME_NO_TEST_CLEANUP=false

# === NOTES IMPORTANTES ===

# 1. SÉCURITÉ:
#    - Ne JAMAIS committer ce fichier avec de vraies valeurs
#    - Utiliser des secrets forts en production (>32 caractères)
#    - Changer tous les secrets par défaut

# 2. ENVIRONNEMENTS:
#    - Development: Secrets optionnels, valeurs par défaut OK
#    - Testing: Utilise des valeurs simulées
#    - Production: TOUS les secrets obligatoires et sécurisés

# 3. TRADING:
#    - ATTENTION: Les clés API Binance donnent accès à vos fonds!
#    - Toujours tester avec testnet avant production
#    - Utiliser des limitations d'IP et de trading

# 4. VARIABLES D'ENVIRONNEMENT:
#    - Préfixe QFRAME_ pour éviter les conflits
#    - Variables sensibles via gestionnaire de secrets en production
#    - Validation automatique au démarrage